programa {
   //Yasmin Teixeira
   inteiro idade 

  funcao inicio() {
    escreva("Digite sua idade:")
    leia(idade)
    se(idade <=12)
    escreva("Classificação: Criança")
    se(idade <=17)
    escreva("Classificação: Adolescente")
    se(idade >17)
    escreva("Classificação: Adulto")
    
    
  }
}
