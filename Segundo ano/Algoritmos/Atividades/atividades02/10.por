programa {
  
  real altura, largura, area, tinta

  funcao inicio() {
    escreva("Qual a altura da parede?")
    leia(altura)
    escreva("Qual  a largura da parede?")
    leia(largura)
    area= largura*altura
    tinta=area*2
    escreva("A área a ser pintada é ",area," e a tinta a ser usada é ",tinta," litros.")

    
  }
}
